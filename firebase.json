{
  "functions": {
    "source": "frontend/functions",
    "runtime": "nodejs20"
  },
  "hosting": {
    "public": "frontend/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/auth/google/callback",
        "function": "authCallback"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
